        "body": [
            "#",
            "# create and fit a spline regression model:",
            "#",
            "# references:",
            "# https://scikit-learn.org/stable/modules/generated/sklearn.pipeline.make_pipeline.html",
            "# https://scikit-learn.org/stable/modules/generated/sklearn.preprocessing.SplineTransformer.html",
            "# https://scikit-learn.org/stable/modules/generated/sklearn.linear_model.LinearRegression.html",           
            "#",
            "from sklearn.pipeline import make_pipeline",
            "from sklearn.preprocessing import SplineTransformer",
            "from sklearn.linear_model import LinearRegression",
            "",
            "${1:estimator_spline} = make_pipeline(",
            "    SplineTransformer(",
            "        n_knots=${2:5},",
            "        degree=${3:3},",
            "        knots='${4|uniform,quantile|}',",
            "        extrapolation='${5|constant,linear,continue,periodic,error|}',",
            "        include_bias=False",
            "    ),",
            "    LinearRegression(",
            "        fit_intercept=${6|True,False|},",
            "        positive=${7|False,True|}",
            "    )",
            ")",
            "$1.fit(X=${8:X},y=${9:y})",
            "$0"
        ]
