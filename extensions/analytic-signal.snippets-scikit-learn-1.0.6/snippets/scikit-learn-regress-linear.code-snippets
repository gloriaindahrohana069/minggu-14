            "from sklearn.pipeline import make_pipeline",
            "from sklearn.decomposition import PCA",
            "from sklearn.linear_model import LinearRegression",
            "",
            "${1:estimator_pcr} = make_pipeline(",
            "    PCA(",
            "        n_components=${2:2},",
            "        whiten=${3|False,True|}",
            "    ),",
            "    LinearRegression(",
            "        fit_intercept=${4|True,False|},",
            "        positive=${5|False,True|}",
            "    )",
            ")",
            "$1.fit(X=${6:X},y=${7:y})",
            "$0"
