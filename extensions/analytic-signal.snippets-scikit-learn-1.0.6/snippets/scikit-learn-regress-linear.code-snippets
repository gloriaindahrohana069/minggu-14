            "# create and fit Linear (OLS) regression model with transformed target (y):",
            "#",
            "# references:",
            "# https://scikit-learn.org/stable/modules/generated/sklearn.compose.TransformedTargetRegressor.html",
            "# https://scikit-learn.org/stable/modules/generated/sklearn.linear_model.LinearRegression.html",           
            "#",
            "from sklearn.compose import TransformedTargetRegressor",
            "from sklearn.linear_model import LinearRegression",
            "",
            "${1:estimator_transform_target} = TransformedTargetRegressor(",
            "    ${2|func=np.reciprocal\\,inverse_func=np.reciprocal,func=np.sqrt\\,inverse_func=np.square,func=np.cbrt\\,inverse_func=lambda x: np.power(x\\,3),func=np.square\\,inverse_func=np.sqrt,func=lambda x: np.power(x\\,3)\\,inverse_func=np.cbrt,func=np.exp\\,inverse_func=np.log,func=np.log\\,inverse_func=np.exp,func=np.expm1\\,inverse_func=np.log1p,func=np.log1p\\,inverse_func=np.expm1,func=np.exp2\\,inverse_func=np.log2,func=np.log2\\,inverse_func=np.exp2,func=np.log10\\,inverse_func=lambda x: np.power(10\\,x),func=np.sin\\,inverse_func=np.asin,func=np.cos\\,inverse_func=np.acos,func=np.tan\\,inverse_func=np.atan,func=np.arcsin\\,inverse_func=np.sin,func=np.arccos\\,inverse_func=np.cos,func=np.arctan\\,inverse_func=np.tan|},",
            "    regressor = LinearRegression(",
            "        fit_intercept=${3|True,False|},",
            "        positive=${4|False,True|}",
            "    ),",
            "    check_inverse=${5|True,False|}",            
            ")",
            "$1.fit(X=${6:X},y=${7:y})",
