    "linear regression with transformed features": {
        "prefix": "sk-regress-linear-transform",
        "description": "Create and fit Linear (OLS) regression model with transformed features (X)",
        "body": [
            "#",
            "# create and fit Linear (OLS) regression model with transformed features (X):",
            "#",
            "# references:",
            "# https://scikit-learn.org/stable/modules/generated/sklearn.pipeline.make_pipeline.html",
            "# https://scikit-learn.org/stable/modules/generated/sklearn.preprocessing.FunctionTransformer.html",
            "# https://scikit-learn.org/stable/modules/generated/sklearn.linear_model.LinearRegression.html",           
            "#",
            "from sklearn.pipeline import make_pipeline",
            "from sklearn.preprocessing import FunctionTransformer",
            "from sklearn.linear_model import LinearRegression",
            "",
            "${1:estimator_transform} = make_pipeline(",
            "    FunctionTransformer(",
            "        func=${2|np.reciprocal,np.sqrt,np.cbrt,np.square,np.exp,np.expm1,np.exp2,np.log,np.log10,np.log2,np.log1p,np.sin,np.cos,np.tan,np.arcsin,np.arccos,np.arctan|}",
            "    ),",
            "    LinearRegression(",
            "        fit_intercept=${3|True,False|},",
            "        positive=${4|False,True|}",
            "    )",
            ")",
            "$1.fit(X=${5:X},y=${6:y})",
            "$0"
        ]
    },
